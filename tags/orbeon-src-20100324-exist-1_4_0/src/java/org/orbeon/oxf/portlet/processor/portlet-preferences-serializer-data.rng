<element name="portlet-preferences" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"
    xmlns="http://relaxng.org/ns/structure/1.0">
    <zeroOrMore>
        <element name="preference">
            <element name="name">
                <data type="NCName"/>
            </element>
            <zeroOrMore>
                <element name="value">
                    <data type="string"/>
                </element>
            </zeroOrMore>
        </element>
    </zeroOrMore>
</element>
